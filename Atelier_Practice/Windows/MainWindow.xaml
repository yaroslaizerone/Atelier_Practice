<Window x:Class="Atelier_Practice.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Atelier_Practice"
        mc:Ignorable="d"
        Icon="F:/ярослав/коды/еу/Atelier_Practice/Atelier_Practice/Resource/Icons/IconDress.png"
        Title="MainWindow" Height="450" Width="800" MinHeight="400" MinWidth="600" MaxHeight="900" MaxWidth="1200">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0" Background="#FA8072">
            <Button x:Name="btnBack" HorizontalAlignment="Right" Content="Назад" Click="btnBack_Click"/>
            <Image Width="45" Height="45" HorizontalAlignment="Left" Margin="5" Source="F:/ярослав/коды/еу/Atelier_Practice/Atelier_Practice/Resource/Icons/IconDress.png"/>
        </Grid>
        <Frame x:Name="frmMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="frmMain_ContentRendered"/>
    </Grid>
</Window>
